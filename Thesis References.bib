	pages = {42--100},
	file = {ScienceDirect Full Text PDF:/Users/will.ngiam/Zotero/storage/GVDMTEEZ/Cowan et al. - 2005 - On the capacity of attention Its estimation and i.pdf:application/pdf;ScienceDirect Snapshot:/Users/will.ngiam/Zotero/storage/DTBZVYRS/S0010028505000022.html:text/html}
