	pages = {420--434},
	file = {Full Text PDF:/Users/will.ngiam/Zotero/storage/T7P35ARW/Portrat et al. - 2015 - Promoting the experimental dialogue between workin.pdf:application/pdf;Snapshot:/Users/will.ngiam/Zotero/storage/N583XPW9/s13421-015-0572-9.html:text/html}
}

