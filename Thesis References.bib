	file = {Full Text PDF:/Users/will.ngiam/Zotero/storage/XJCMDD5K/Kaiser et al. - 2015 - Real-world spatial regularities affect visual work.pdf:application/pdf;Snapshot:/Users/will.ngiam/Zotero/storage/RSVR442Z/s13423-015-0833-4.html:text/html}
}

@article{logie_bound_2009,
